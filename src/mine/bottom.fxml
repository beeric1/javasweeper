<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="mine.BottomController"
      prefHeight="0.0" prefWidth="200.0" spacing="2.0" BorderPane.alignment="CENTER_LEFT">
<children>
    <HBox prefHeight="100.0" prefWidth="80.0" spacing="5.0">
        <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mines left:" />
            <Text fx:id="mines" strokeType="OUTSIDE" strokeWidth="0.0" text="0" />
        </children>
    </HBox>
    <HBox prefHeight="100.0" prefWidth="0.0" spacing="5.0">
        <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time:" />
            <Text fx:id="time" strokeType="OUTSIDE" strokeWidth="0.0" text="0" />
        </children>
    </HBox>
</children>
<padding>
    <Insets bottom="2.0" left="5.0" right="5.0" top="2.0" />
</padding>
</HBox>
